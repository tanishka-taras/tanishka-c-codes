#include<stdio.h>
void main(){
    int i,j,c;
int  a[10]={30,100,47,39,55,90,58,108,78,54,};

    for(i=0;i<10;i++)
{
    for (j=i+1;j<10;j++)
    {

if(a[j]>a[i]){
    c=a[i];
    a[i]=a[j];
    a[j]=c;
}
    }
}
printf("printing sorted element list \n");
for(i=0;i<10;i++)
{
    printf("%d\n",a[i]);
}
}


    
